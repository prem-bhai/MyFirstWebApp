@{
    Layout = "_Layout";
}
<h2>Login</h2>
<form method="post" asp-controller="Account" asp-action="Login">
    <div><label>Email</label><input name="email" /></div>
    <div><label>Password</label><input name="password" type="password" /></div>
    <div><button type="submit">Login</button></div>
</form>
